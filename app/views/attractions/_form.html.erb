<%= form_for [@itinerary, @attraction] do |f| %>
  <div class = "form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control"%><br>
  </div>
  <div class = "form-group">
    <%= f.label :address %><br>
    <%= f.text_field :address, class: "form-control"%><br>
  </div>
  <div class = "actions"><br>
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>