<%= form_for [@itinerary, @restaurant] do |f| %>
  <div class = "form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control"%><br>
  </div>
  <div class = "form-group">
    <%= f.label :location %><br>
    <%= f.text_field :location, class: "form-control"%><br>
  </div>
  <div class = "form-group">
    <%= f.label :review %><br>
    <%= f.text_area :review, class: "form-control"%><br>
  </div>
  <div class = "form-group">
    <%= f.label :date %><br>
    <%= f.date_field :date %><br>
  </div>

  <div class = "actions"><br>
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>