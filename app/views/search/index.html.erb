<% if @results.empty? %>
  <%= "No results matching that query." %>
  <% else %>
<div class="panel panel-primary">
  <div class="panel-heading">Search Results</div>
  <div class="panel-body">
    <div class="list-group">
      <% @results.each do |result| %>
        <% if result.class == Itinerary %>
          <h4><%= link_to ("#{"("+result.class.to_s+") " + result.title}"), result, class:"list-group-item" %> </h4><br>
        <% else %>
          <h4><%= link_to ("#{"("+result.class.to_s+") " + result.name}"), result, class:"list-group-item" %> </h4><br>
        <% end %>
      <% end %>
    </div>
    <% end %>

  </div>
</div>